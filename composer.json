{
  "name": "treehouselabs/:package_name",
  "type": "symfony-bundle",
  "description": ":bundle_description",
  "keywords": [],
  "license": "MIT",
  "authors": [
    {
      "name": ":author_name",
      "email": "author@treehouse.nl",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "symfony/symfony": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.0",
    "mockery/mockery": "^0.9"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\:bundle_namespace\\": "src/TreeHouse/:bundle_namespace/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "TreeHouse\\:bundle_namespace\\Tests\\": "tests/src/TreeHouse/:bundle_namespace/Tests/"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
